import numpy as np

x= np.array([3.14,10,5,20,85])
y=np.arange(9)
#For döngüsü gibi bir nevi
print(x-2)
print(y)

#Output:
[ 1.14  8.    3.   18.   83.  ]
[0 1 2 3 4 5 6 7 8]
-------------------------------
import numpy as np

x= np.array([3.14,10,5,20,85])
print(x.max())
print(x.min())
print(np.sqrt(x))
print(np.cos(x))
